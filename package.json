{
  "name": "eosjs2",
  "version": "0.0.13",
  "description": "Talk to eos API",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p ./tsconfig.node.json && cp src/ripemd.js dist && babel dist --out-dir dist",
    "build-web": "webpack",
    "docs-build": "./node_modules/.bin/typedoc ./src --options typedoc.json && ./node_modules/.bin/gitbook build ./docs/build",
    "docs-serve": "./node_modules/.bin/gitbook serve ./docs/build"
  },
  "author": "block.one",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EOSIO/eosjs2.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "eosjs-ecc": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^10.3.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-1": "^6.24.1",
    "gitbook-cli": "2.3.2",
    "json-loader": "^0.5.7",
    "ts-loader": "^4.3.1",
    "typedoc": "0.11.1",
    "typedoc-plugin-markdown": "1.1.13",
    "typescript": "^2.9.1",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2"
  }
}
